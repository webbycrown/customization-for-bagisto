{
    "name": "webbycrown/customization-for-bagisto",
    "description": "Dynamic page customization system for Bagisto - Create and manage page sections, fields, and content from the backend with API support",
    "type": "library",
    "version": "1.0.1",
    "license": "MIT",
    "keywords": [
        "bagisto",
        "customization",
        "page-builder",
        "cms",
        "laravel",
        "e-commerce",
        "webbycrown"
    ],
    "authors": [
        {
            "name": "WebbyCrown Solutions",
            "email": "info@webbycrown.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2",
        "intervention/image": "^2.7|^3.0"
    },
    "suggest": {
        "intervention/imagecache": "For image caching support (optional)"
    },
    "autoload": {
        "psr-4": {
            "Webbycrown\\Customization\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webbycrown\\Customization\\Providers\\CustomizedServiceProvider"
            ],
            "aliases": {}
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
